@model IEnumerable<SongViewModel>

<div class="card">
    <div class="card-body">
        <div class="row">
            @foreach (var song in Model)
            {
                <div class="col-6 col-lg-2 col-sm-4">
                    @await Component.InvokeAsync("Song", song)
                </div>
            }
        </div>
    </div>
</div>